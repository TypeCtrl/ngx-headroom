{
  "name": "@ctrl/ngx-headroom",
  "version": "1.1.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ts-node build.ts",
    "publish": "node prepublish.js && cd dist && npm publish --access public",
    "test": "ng test --browsers=ChromeCI",
    "test:ci": "ng test --single-run --code-coverage --no-progress --browsers=ChromeCI",
    "lint": "ng lint --type-check -t stylish",
    "lint:fix": "ng lint --fix --type-check -t stylish",
    "e2e": "ng e2e",
    "bundlesize": "npx bundlesize",
    "ghpages": "ng build --prod --no-progress --baseHref='/ngx-headroom/'"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "5.2.10",
    "@angular/common": "5.2.10",
    "@angular/compiler": "5.2.10",
    "@angular/core": "5.2.10",
    "@angular/platform-browser": "5.2.10",
    "@angular/platform-browser-dynamic": "5.2.10",
    "@ctrl/ngx-github-buttons": "1.0.9",
    "bootstrap": "4.0.0",
    "core-js": "2.5.5",
    "rxjs": "5.5.10",
    "web-animations-js": "2.3.1",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "1.7.4",
    "@angular/compiler-cli": "5.2.10",
    "@angular/language-service": "5.2.10",
    "@types/jasmine": "2.8.6",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "9.6.6",
    "codelyzer": "4.2.1",
    "cpx": "1.5.0",
    "jasmine-core": "2.99.1",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "2.0.2",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.4.2",
    "karma-jasmine": "1.1.1",
    "karma-jasmine-html-reporter": "0.2.2",
    "ng-packagr": "2.4.2",
    "protractor": "5.3.1",
    "rimraf": "2.6.2",
    "ts-node": "6.0.0",
    "tslint": "5.9.1",
    "typescript": "2.6.2"
  },
  "bundlesize": [
    {
      "path": "./dist/bundles/ctrl-ngx-headroom.umd.js",
      "maxSize": "3 kB"
    },
    {
      "path": "./dist/bundles/ctrl-ngx-headroom.umd.min.js",
      "maxSize": "2 kB"
    },
    {
      "path": "./dist/esm5/ctrl-ngx-headroom.js",
      "maxSize": "3 kB"
    },
    {
      "path": "./dist/esm2015/ctrl-ngx-headroom.js",
      "maxSize": "3 kB"
    }
  ]
}
