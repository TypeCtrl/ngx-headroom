{
  "name": "@ctrl/ngx-headroom",
  "version": "1.1.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ts-node build.ts",
    "publish": "node prepublish.js && cd dist && npm publish --access public",
    "test": "ng test --browsers=ChromeCI",
    "test:ci": "ng test --watch=false --code-coverage --no-progress --browsers=ChromeCI",
    "lint": "ng lint ngx-headroom --type-check ---format stylish",
    "lint:fix": "ng lint ngx-headroom --fix --type-check ---format stylish",
    "e2e": "ng e2e",
    "bundlesize": "npx bundlesize",
    "ghpages": "ng build --prod --no-progress --baseHref='/ngx-headroom/'"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.0.0",
    "@angular/common": "6.0.0",
    "@angular/compiler": "6.0.0",
    "@angular/core": "6.0.0",
    "@angular/platform-browser": "6.0.0",
    "@angular/platform-browser-dynamic": "6.0.0",
    "@ctrl/ngx-github-buttons": "2.0.2",
    "bootstrap": "4.1.1",
    "core-js": "2.5.5",
    "rxjs": "6.1.0",
    "web-animations-js": "2.3.1",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "6.0.0",
    "@angular/compiler-cli": "6.0.0",
    "@angular/language-service": "6.0.0",
    "@types/jasmine": "2.8.7",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "10.0.4",
    "codelyzer": "4.3.0",
    "jasmine-core": "3.1.0",
    "karma": "2.0.2",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.4.2",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "0.2.2",
    "ng-packagr": "3.0.0-rc.3",
    "rimraf": "2.6.2",
    "ts-node": "6.0.3",
    "tslint": "5.10.0",
    "typescript": "2.7.2",
    "@angular-devkit/build-angular": "~0.6.0"
  },
  "bundlesize": [
    {
      "path": "./dist/bundles/ctrl-ngx-headroom.umd.js",
      "maxSize": "3 kB"
    },
    {
      "path": "./dist/bundles/ctrl-ngx-headroom.umd.min.js",
      "maxSize": "2 kB"
    },
    {
      "path": "./dist/esm5/ctrl-ngx-headroom.js",
      "maxSize": "3 kB"
    },
    {
      "path": "./dist/esm2015/ctrl-ngx-headroom.js",
      "maxSize": "3 kB"
    }
  ]
}
